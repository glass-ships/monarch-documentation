schema:Thing a sh:NodeShape ;
    sh:closed true ;
    sh:description "A generic grouping for any identifiable entity" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A unique identifier for a resource" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ],
        [ sh:description "A URL to the documentation about this asset." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path <https://w3id.org/monarch-initiative/monarch-technical-documentation/:documentation> ],
        [ sh:description "A human-readable description for a resource" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:description ],
        [ sh:description "The role the Monarch Initiative plays in the maintainence of this asset." ;
            sh:in ( "Lead" ) ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path <https://w3id.org/monarch-initiative/monarch-technical-documentation/:monarch_contribution> ],
        [ sh:description "A human-readable name for a resource" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema:name ],
        [ sh:description "The grant used to finance the production of an asset." ;
            sh:in ( "Phenomics First" "Monarch R24" "HPO" "Exomiser" ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path <https://w3id.org/monarch-initiative/monarch-technical-documentation/:grant> ] ;
    sh:targetClass schema:Thing .

